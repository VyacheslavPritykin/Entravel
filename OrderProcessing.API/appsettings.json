{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "MassTransit": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "OutboxOptions": {
    "PollingInterval": "00:00:05",
    "BatchSize": 20
  }
}
